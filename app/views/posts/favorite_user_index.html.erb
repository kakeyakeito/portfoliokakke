<%= render "layouts/user_header"%>
<div class="container">
	<div class="row">
		<h1>いいねしているUser</h1>
		<div class="col-md-3"></div>
		<div class="col-md-6">
			<% @users.each do |user| %>
			<div class="user_favorite_index">
				<div class="username">
					<% if current_user == user %>
						<%= link_to "#{user.name}", mypage_user_path(current_user) %>
					<% else %>
						<%= link_to "#{user.name}", user_path(user) %>
					<% end %>
				</div>
			</div>
			<% end %>
		</div>
		<div class="col-md-3"></div>
	</div>
</div>